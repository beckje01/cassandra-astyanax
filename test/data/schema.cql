DROP TABLE User;
CREATE TABLE User (KEY varchar PRIMARY KEY, id varchar, city varchar, state varchar, zip varchar, e164phone varchar);
CREATE INDEX city_idx ON User(city);
CREATE INDEX state_idx ON User(state);
CREATE INDEX zip_idx ON User(zip);
CREATE INDEX e164phone_idx ON User(e164phone);
DROP TABLE User_CTR;
CREATE TABLE User_CTR (KEY varchar PRIMARY KEY) WITH default_validation=CounterColumnType;
DROP TABLE Standard1;
CREATE TABLE Standard1 (KEY varchar PRIMARY KEY) WITH comparator=UTF8Type AND default_validation=UTF8Type;
DROP TABLE Counter1;
CREATE TABLE Standard1 (KEY varchar PRIMARY KEY) WITH comparator=UTF8Type AND default_validation=CounterColumnType;